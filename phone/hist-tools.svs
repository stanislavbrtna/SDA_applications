function add_to_hist {
  if(DEBUG) print("add_to_hist: adding call to history!");
  
  sys.fs.chDir();
  sys.fs.db.open("appdata/phone/history.db");
  sys.fs.db.selectTable("history");
  
  sys.fs.db.newRow();
  
  sys.fs.db.setEntryStr("number", callNumber);
  sys.fs.db.setEntryStr("name", callName);
  sys.fs.db.setEntryNum("start", callStart);
  sys.fs.db.setEntryNum("stop", callEnd);
  sys.fs.db.setEntryStr("type", callType);
  
  sys.fs.db.close();
}

